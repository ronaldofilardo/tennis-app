{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "prisma generate && tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest run --coverage --reporter=json",
    "test:regression": "vitest run regression.test.jsx",
    "test:update-mocks": "node scripts/update-mocks.js",
    "test:coverage": "node --max-old-space-size=4096 ./node_modules/vitest/vitest.mjs run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install",
    "prisma:generate": "prisma generate",
    "dev-server": "node dev-server.cjs",
    "test-server": "node test-server.cjs",
    "test:mutation": "stryker run"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "axios": "^1.6.0",
    "pg": "^8.16.3",
    "prisma": "^6.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.30.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@stryker-mutator/core": "^9.3.0",
    "@stryker-mutator/vitest-runner": "^9.3.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.0",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.2.7",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "express": "^4.18.2",
    "jsdom": "^27.1.0",
    "typescript": "^5.0.2",
    "vite": "^4.5.14",
    "vitest": "^4.0.7"
  }
}
